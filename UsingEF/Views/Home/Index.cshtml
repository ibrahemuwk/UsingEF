@model ViewModel
@{
    ViewBag.Title = "Index";
}

<table class="table-striped">
    <thead>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Country</th>

    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.uers)
    {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.Name
            </td>
            <td>
                @item.Country
            </td>
        </tr>
    }
    </tbody>
</table>
<div class="btn btn-primary"><a href="../Home/New">Add User</a></div>
